{
  "evals": [
    {
      "id": "eval-1-simple-crud",
      "name": "Simple CRUD Resource",
      "prompt": "Use devteam to build a Product catalog with Filament admin. Products should have: name, description, price, stock, category (relation), and image. Include basic CRUD operations.",
      "expectations": [
        {
          "type": "file_exists",
          "path": "planning/REQUIREMENTS.md",
          "description": "Planning phase should create requirements document"
        },
        {
          "type": "file_exists",
          "path": "planning/ARCHITECTURE.md",
          "description": "Planning phase should create architecture document"
        },
        {
          "type": "file_exists",
          "path": "planning/DATA_MODEL.md",
          "description": "Planning phase should create data model document"
        },
        {
          "type": "content_contains",
          "path": "app/Models/Product.php",
          "content": "class Product extends Model",
          "description": "Should create Product model"
        },
        {
          "type": "content_contains",
          "path": "app/Filament/Resources/ProductResource.php",
          "content": "class ProductResource extends Resource",
          "description": "Should create Filament resource"
        },
        {
          "type": "file_exists",
          "path": "tests/Unit/Models/ProductTest.php",
          "description": "Should create model tests"
        },
        {
          "type": "file_exists",
          "path": "tests/Feature/Filament/ProductResourceTest.php",
          "description": "Should create Filament resource tests"
        },
        {
          "type": "file_exists",
          "path": "docs/api/products.md",
          "description": "Should create API documentation"
        },
        {
          "type": "checkpoint_approval",
          "phase": "planning",
          "description": "Should present planning results for approval"
        },
        {
          "type": "checkpoint_approval",
          "phase": "development",
          "description": "Should present development results for approval"
        },
        {
          "type": "checkpoint_approval",
          "phase": "testing",
          "description": "Should present testing results for approval"
        }
      ],
      "tags": ["basic", "crud", "filament"],
      "estimated_tokens": 400000,
      "estimated_cost_usd": 20
    },
